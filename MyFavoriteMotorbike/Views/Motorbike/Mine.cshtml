@using System.Collections.Generic
@model IEnumerable<MotorbikeServiceModel>

@{
    ViewBag.Title = "My Motorbikes";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any()) 
{
    <div class="row">
        @foreach (var motorbike in Model)
        {
            <partial name="_MotorbikePartial" model="@motorbike" />
        }
    </div>
}
else
{
    <h3 class="text-center">You have no motorbikes yet!</h3>
}